#!/bin/bash

# Load resources

xrdb -merge .Xresources

export GTK_OVERLAY_SCROLLING=0
# Fire up apps
urxvtd -q -o -f &
/home/mindaugas/.scripts/wallpaper.sh &
setxkbmap -layout "us,lt,ru" -option "grp:ctrl_shift_toggle" &

if [ -x /usr/bin/nm-applet ] ; then
   nm-applet --sm-disable &
fi

exec  xmonad
